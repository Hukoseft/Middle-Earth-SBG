    <profile id="e2ca-1f1b-e652-73a8" name="*Staff of Power" book="Middle-Earth Strategy Battle Game Rules Manual" page="86" hidden="false" profileTypeId="c6d6-90e7-ef64-8355" profileTypeName="Wargear">
      <profiles/>
