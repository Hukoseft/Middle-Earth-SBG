    <profile id="8465-2ce3-b52a-1c08" name="War Spear" book="Middle-Earth Strategy Battle Game Rules Manual" page="85" hidden="false" profileTypeId="c6d6-90e7-ef64-8355" profileTypeName="Wargear">
